{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "dashboard imu",
  "productName": "dashboard imu",
  "author": "Matheus Rossetti <matheus__rossetti@hotmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "generate:api": "orval",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "cross-env NODE_ENV=development quasar dev",
    "build": "cross-env NODE_ENV=production quasar build",
    "postinstall": "quasar prepare",
    "dev:browser": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build:browser": "quasar build",
    "build:electron": "quasar build -m electron",
    "build:debug": "quasar build -m electron --debug"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "orval": "^7.10.0",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "three": "^0.176.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/three": "^0.176.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "sonar-scanner": "^3.1.0",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
